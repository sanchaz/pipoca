# Cálculo dos divisores de um número
start()
  var n
  'Introduza um número inteiro positivo: '!
  if n? <> 1 or n <= 0 then
    start := 1
  else
    if n > 3 then
      var i, div
      div := 0
      i := 2
      while i <= n/2 do
	if n % i = 0 then
	  div := div + 1
	  if div = 1 then
	    'O número '+n+' é divisível por:'!
	  endif
	  ' '+i!
	endif
      done
    endif
    if div = 0 then
      'O número '+n+' é primo'!
    endif
    ''!!
    start := 0
  endif
end

